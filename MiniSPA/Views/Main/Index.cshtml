@model MiniSPA.Models.MainModel

@{
    ViewBag.Title = "SPA";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section JSInHead{
    <script src="http://cdn.kendostatic.com/2015.1.429/js/kendo.all.min.js"></script>
    <script type="text/javascript">
        customerModule.factory('bootstrappedData', function () {
            return {
                customers:@Html.Raw(Model.customer),
                instructors:@Html.Raw(Model.instModel)
                };
        });
    </script>
}

<div class="container">
    <div class="row">
        <div class="navbar navbar-default">
            <div class="navbar-header">
                <ul class="nav navbar-nav">
                    <li><span class="navbar-brand">Registration</span></li>
                </ul>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a href="/Main/Customers">Browse Customers</a></li>
                    <li><a href="/Main/Instructors">Browse Instructors</a></li>
                </ul>
            </div>
        </div>
    </div>
    <div ng-view></div>
</div>


